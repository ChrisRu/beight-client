{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/prop-types": 0,
    "react/no-unknown-property": 0,
    "react/jsx-filename-extension": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "arrow-parens": ["error", "as-needed"],
    "comma-dangle": ["error", "never"]
  },
  "env": {
    "browser": true
  }
}
